<templateSet group="Handlebars">
  <template name="{{#if" value="{{#if $CONDITION$}}&#10;$SELECTION$&#10;{{/if}}" description="Surround with If block" toReformat="false" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="&quot;condition&quot;" alwaysStopAt="true" />
