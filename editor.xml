<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="LiveEditOptions" useJBChromeExtension="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>